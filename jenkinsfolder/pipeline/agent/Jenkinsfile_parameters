pipeline{
    agent any
    stages {
        stage ('BUILD') {
            steps {
                script {
                    def ENVIRONMENT = "${params.Deploy}"
                    if(ENVIRONMENT == 'Test'){
                        echo "Deploying to Test......"
                    
                    } else if(ENVIRONMENT == 'QA'){
                        echo "Deploying to QA......"
                    }
                }
            }
        }
    }
}
